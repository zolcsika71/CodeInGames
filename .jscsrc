{
	"preset": "crockford",
        "maximumLineLength": 240,
        "requireCamelCaseOrUpperCaseIdentifiers": false,
        "validateIndentation": 4,
		"maxErrors": 1000,
        "requireCurlyBraces": [
          "while",
          "do",
          "try",
          "catch",
          "switch"
        ],
        "disallowKeywords": [""],
        "disallowDanglingUnderscores": { "allExcept": ["", ""] }
}
